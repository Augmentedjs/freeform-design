<h2>Function Routes</h2>
<div id="functionRoutes">
    <ul>
        {{#each functionRoutes}}
        <li>
            <div class="flexContainer">
                <p>{{route}}<br/><span class="secondary">{{callback}}</span></p>
                <div>
                    <div class="inlineButton large" data-index="{{@index}}" data-routes="edit" data-click="editRoute"><i class="material-icons md-dark">edit</i></div>
                </div>
            </div>
        </li>
        {{/each}}
    </ul>
</div>
