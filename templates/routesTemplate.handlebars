<ul>
    {{#each currentRoutes}}
    <li>
        <div class="flexContainer">
            <p>{{route}}<br/>
                <span class="secondary">{{callback}}</span><br/>
                <span class="secondary">{{type}}</span>
            </p>
            <div>
                <div class="inlineButton large" data-index="{{@index}}" data-routes="edit" data-click="editRoute"><i class="material-icons md-dark">edit</i></div>
            </div>
        </div>
    </li>
    {{/each}}
</ul>
